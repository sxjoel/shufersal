<div>
  <table class="table" >
    <thead>
    <tr>
      <th>שם המוצר</th>
      <th>מחיר</th>
      <th>מזמינים</th>
      <th>כמות</th>
    </tr>
    </thead>

    <% @products.each do |product| %>
        <tr>
         <td><%= product['product']['name'] %></td>
         <td><%= product['product']['price'] %></td>
         <td><%= product['user_names'].join(",") %></td>
         <td><%= product['product']['quantity'] %></td>
        </tr>
    <% end %>
  </table>

</div>
